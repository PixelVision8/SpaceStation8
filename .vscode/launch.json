    "version": "0.2.0",
    "configurations": [
        {
            "name": "Space Station 8 (Debug)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "gulp-build",
            "program": "${workspaceFolder}/App/bin/Debug/net5.0/SpaceStation8.dll",
            "args": ["Game"],
            "cwd": "${workspaceFolder}/",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}